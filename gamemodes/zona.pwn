stock SaveZones()
{
	new idx;
	new File: file2;
	while (idx < 5)
	{
		new coordsstring[512];
		format(coordsstring, sizeof(coordsstring), "%d|%d\n",
		gangzone[idx][mainzones],
		gangzone[idx][mainzones2]);
		if(idx == 0) file2 = fopen("gangzones.cfg", io_write);
