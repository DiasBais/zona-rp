stock GetRPName(name[])
{
	new Left[32],Right[32];
	if(strfind(name,"_",true,1) == -1) return false;

	for(new i ; i < strlen(name); i++)
	if( name[i] <= '9' && name[i] >= '0') return false;

	new pos = strfind(name,"_",true,-1);
	strmid(Left, name, 0, pos,32);
	strmid(Right, name, pos+1,32,32);

	if(  !(Right[0] >='A' && Right[0] <= 'Z' && Left[0] >='A'&& Left[0] <= 'Z') ) return false;

	if(strcmp(Right, Left, true) == 0) return false;

	return true;
}

