public OnPlayerSpawn(playerid)
{
	StopAudioStreamForPlayer(playerid);
	SetPVarInt(playerid,"K_Times",0);
	Camera[playerid] = 0;//Чтобы летящая камера остоновилась.
	Masked[playerid] = 0;
	TeleportTime[playerid] = 4;
	Pursiut[playerid] = -1;
	AntiFakeKill[playerid] = true;
	SetPVarInt(playerid, "NoAB", 1);
	if(gPlayerLogged[playerid] == 0)
	{
		SendClientMessage(playerid, COLOR_YELLOW, "Необходимо авторизоваться!");
		KickEx(playerid);
		return true;
	}
	if(PlayerInfo[playerid][pMember] > 0)
	{
		SetPlayerSkin(playerid, PlayerInfo[playerid][pModel]);
	}
	if(PlayerInfo[playerid][pMember] <= 0)
	{
		SetPlayerSkin(playerid, PlayerInfo[playerid][pChar]);
	}
	SetPlayerSkills(playerid);
	SetPlayerSpawn(playerid);
	TextDrawHideForPlayer(playerid,TvTextDraw[playerid]);
	return true;
}
