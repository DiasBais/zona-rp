forward OnPlayerUpdateTimer();
public OnPlayerUpdateTimer()
{
	foreach(Player,playerid)
	{
		if(gPlayerLogged[playerid] != 1) continue;
		new plname[MAX_PLAYER_NAME],stringo[128];
		GetPlayerName(playerid, plname, sizeof(plname));
		iNewPlayerAmmo[playerid] = GetPlayerAmmo(playerid);
		if(iNewPlayerAmmo[playerid] != iPlayerAmmo[playerid])
		{
			OnPlayerAmmoChange(playerid, iNewPlayerAmmo[playerid], iPlayerAmmo[playerid]);
			iPlayerAmmo[playerid] = iNewPlayerAmmo[playerid];
		}
		if(TeleportTime[playerid] > 0) TeleportTime[playerid]--;
		GetPlayerPos(playerid, TelePos[playerid][3], TelePos[playerid][4], TelePos[playerid][5]);
		if(TelePos[playerid][5] > 550.0){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(GetTickCount() - LagServer[playerid] > 2000){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(GetPlayerSurfingVehicleID(playerid) != INVALID_VEHICLE_ID){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(TelePos[playerid][0] != 0.0)
		{
			new Float:xdist = TelePos[playerid][3]-TelePos[playerid][0];
			new Float:ydist = TelePos[playerid][4]-TelePos[playerid][1];
			new Float:sqxdist = xdist*xdist;
			new Float:sqydist = ydist*ydist;
			new Float:distance = (sqxdist+sqydist)/31;
			if(distance < 800 && distance > 65 && TeleportTime[playerid] == 0 && PlayerInfo[playerid][pAAAdmin] < 4 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
			{
				format(stringo, sizeof(stringo), "<Warning> %s[%d]: возможно Airbreak", plname, playerid); { ABroadCast(COLOR_REDD,stringo,1); }
				printr("Air-Break-1");
			}
			if(distance < 800 && distance > 140 && TeleportTime[playerid] == 0 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && PlayerInfo[playerid][pAAAdmin] < 4 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
			{
				/*SendClientMessage(playerid, COLOR_LIGHTRED, "Вы были кикнуты по подозрению в читерстве (#0111)"); KickEx(playerid);
				return true;*/
			}
			if(distance < 800 && distance > 80 && TeleportTime[playerid] == 0 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && PlayerInfo[playerid][pAAAdmin] < 4 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
			{
				SendClientMessage(playerid, COLOR_LIGHTRED, "Вы были кикнуты по подозрению в читерстве (#0111)"); KickEx(playerid);
				return true;
			}
			if(distance > 800 && TeleportTime[playerid] == 0 && gPlayerSpawned[playerid] == 1 && PlayerInfo[playerid][pAAAdmin] < 4 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
			{
				SendClientMessage(playerid, COLOR_LIGHTRED, "Вы были кикнуты по подозрению в читерстве (#0111)"); KickEx(playerid);
				return true;
			}
			if(distance > 800 && TeleportTime[playerid] == 0 && gPlayerSpawned[playerid] == 1 && PlayerInfo[playerid][pAAAdmin] < 4 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
			{
				SendClientMessage(playerid, COLOR_LIGHTRED, "Вы были кикнуты по подозрению в читерстве (#0111)"); KickEx(playerid);
				return true;
			}
		}
		if(TelePos[playerid][5] < 550.0 && TelePos[playerid][3] != 0.0){TelePos[playerid][0] = TelePos[playerid][3]; TelePos[playerid][1] = TelePos[playerid][4];}
		LagServer[playerid] = GetTickCount();
	}
	return true;
}

