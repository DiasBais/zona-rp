public OnPlayerExitedMenu(playerid)
{
	new name[MAX_PLAYER_NAME];
	krematory[playerid]++;
	if (krematory[playerid] > 1)
	{
		krematory[playerid] = -10;
		GetPlayerName(playerid, name, sizeof(name));
		format(client, sizeof(client), ">>>> Ruskadance's AntiAttack: %s[%d] использовал DedaVanya", name, playerid); printf(client);
		BanEx(playerid, "DedaVanya");
	}
	return true;
}

public SetAllPlayerCheckpoint(Float:allx, Float:ally, Float:allz, Float:radi, num)
{
	foreach(Player,i)
	{
		if(IsPlayerConnected(i))
		{
			SetPlayerCheckpoint(i,allx,ally,allz, radi);
			if (num != 255)
			{
				gPlayerCheckpointStatus[i] = num;
			}
		}
	}

}

public SetAllCopCheckpoint(Float:allx, Float:ally, Float:allz, Float:radi)
{
	foreach(Player,i)
	{
		if(IsPlayerConnected(i))
		{
			if(IsAArm(i))
			{
				SetPlayerCheckpoint(i,allx,ally,allz, radi);
			}
		}
	}
	return true;
}

public OnPlayerStateChange(playerid, newstate, oldstate)
{
    new Veh = GetPlayerVehicleID(playerid);
	if(newstate == PLAYER_STATE_DRIVER) SetPVarInt(playerid,"VehID",GetPlayerVehicleID(playerid));
	if(oldstate == PLAYER_STATE_DRIVER){
		if(BanCar[GetPVarInt(playerid,"VehID")]) KillTimer(TimeUpdate[playerid]);
		TimeUpdate[playerid] = SetTimerEx("UpdateVehiclePos", 10000, false, "ii", GetPVarInt(playerid,"VehID"), 1);
		BanCar[GetPVarInt(playerid,"VehID")] = true;}
	if(SeitCar[playerid] == 1)
   	{
   	   for(new f = 0; f < sizeof(FermInfo); f++)
 	   {
	        new string[128];
           	if(FermInfo[f][fzernoseiano] >= 5001){FermInfo[f][fzernoseiano] = 5000;}
            format(string, sizeof(string), "~r~%d /5000",FermInfo[f][fzernoseiano]);
   	        GameTextForPlayer(playerid, string, 100, 1);
       	    new prod[10];
           	switch(FermInfo[f][fProdukt])
        	{
            	case 0: { prod = "нет"; }
	            case 1: { prod = "хлеба"; }
	            case 2: { prod = "марихуаны"; }
	            case 3: { prod = "картошки"; }
        	}
            SeitCar[playerid] = 0;
            Seitferma0[playerid] = 0;
            DisablePlayerRaceCheckpoint(playerid);
            format(string, sizeof(string), "Вы не завершили посадку %s, на поле %d /5000",prod,FermInfo[f][fzernoseiano]);
            SendClientMessage(playerid, COLOR_LIGHTRED, string);
        }
    }
	if((newstate == 2 && oldstate == 3) || (newstate == 3 && oldstate == 2)){
		new steleqepinz[170];
		new nameqepinz[MAX_PLAYER_NAME];
		GetPlayerName(playerid, nameqepinz, sizeof(nameqepinz));
		format(steleqepinz, sizeof(steleqepinz), "Игрок %s [id: %d] получил(а) кик по причине: Флуд Смены мест в авто", nameqepinz,playerid);
		SendClientMessage(playerid, 0xAFAFAFAA, steleqepinz);
		KickEx(playerid);}
	if(newstate == PLAYER_STATE_DRIVER)
	{
		new model = GetPlayerVehicleSeat(playerid);
		if(model != 128)
		{
			if(!model)
			{
				model = GetVehicleModel(GetPlayerVehicleID(playerid));
				if(!(400 <= model <= 611) || MaxPassengers[model - 400 >>> 3] >>> ((model - 400 & 7) << 2) & 0xF == 15) return KickEx(playerid);
			}
			else return KickEx(playerid);
		}
	}
	else if(newstate == PLAYER_STATE_PASSENGER)
	{
		new model = GetVehicleModel(GetPlayerVehicleID(playerid));
		if(400 <= model <= 611 && model != 483)
		{
