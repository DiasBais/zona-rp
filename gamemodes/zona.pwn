public OnPlayerCommandText(playerid, cmdtext[])
{
	if(logs == 1)
	{
		printr("OnPlayerCommandText");
	}
	new string[512];
	new specid;
	new playermoney;
	new sendername[MAX_PLAYER_NAME];
	new giveplayer[MAX_PLAYER_NAME];
	new cmd[256];
	new tmp[256];
	new giveplayerid, moneys, idx;
	cmd = strtok(cmdtext, idx);
	GetPlayerName(playerid, sendername, sizeof(sendername));
	if(gPlayerLogged[playerid] == 1 || PlayerInfo[playerid][pAAAdmin] == 0)
	{
		SetPVarInt(playerid, "NoAB", 1);
		if(GetPVarInt(playerid,"AntiFlood") > gettime()){ SendClientMessage(playerid, 0xFFD5BBAA, "Не Нарушайте Чат!"); return true; }
		SetPVarInt(playerid,"AntiFlood",gettime() + ANTI_FLOODS);
	}
	if(gPlayerLogged[playerid] ==0) return true;
	else if(strcmp(cmd, "/shield", true) == 0)
	{
		if(IsPlayerConnected(playerid))
		{
			if(IsACop(playerid) || IsAArm(playerid))
			{
				if(shield[playerid] == 0)
				{
					SetPlayerAttachedObject(playerid, 1, 18637, 14, 0.0, 0.0, 0.0, 0.0, 180.0, 180.0);
					SetPlayerHealth(playerid, 999);
					shield[playerid] = 1;
				}
				else
				{
					RemovePlayerAttachedObject(playerid, 1);
					SetPlayerHealth(playerid, 150);
					shield[playerid] = 0;
				}
			}
			else
			{
				SendClientMessage(playerid, COLOR_WHITE, "{FF0000}x{FFFFFF} Вы не Полицейский/Военный/ФБР");
				return true;
			}
			return true;
		}
	}
